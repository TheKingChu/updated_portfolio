{
    "builds": [
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
        "distDir": "staticfiles_build"
        }
    },
    {
        "src": "/django_portfolio/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
    }
    ],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "django_portfolio/wsgi.py"
    },
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    }
    ]
}